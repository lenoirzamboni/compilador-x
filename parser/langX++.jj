}
{

try{
  [t1 = <MODIFIER>] [t2 = <FINAL>] (t3 = <VAR_TYPE> | t3 = <IDENT>)
    (<LBRACKET> <RBRACKET> { k++; })* t4 = <IDENT> m = methodbody(g)
      {
      if(t1 == null) {
        if(t2 == null) {
          t = t3;
