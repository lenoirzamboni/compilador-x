/* Espacos a serem desprezados no inicio de cada token */

SKIP :
{
    " "
  | "\t"
  | "\n"
  | "\r"
  | "\f"
}

SKIP :
{
    "/*" : multilinecomment
}

SKIP :
{
    "//" : singlelinecomment
}

<multilinecomment> SKIP:
{
    "*/" : DEFAULT
    | <~[]>
}

<singlelinecomment> SKIP:
{
    <["\n","\r"]> : DEFAULT
    | <~[]>
}


/* Palavras reservadas */

TOKEN :
{
    < BREAK: "break" > 
    | < CLASS: "class" >
    | < CONSTRUCTOR: "constructor" >
    | < ELSE: "else" >
    | < EXTENDS: "extends" >
    | < FOR: "for" >
    | < IF: "if" >
    | < NEW: "new" >
    | < PRINT: "print" >
    | < READ: "read" >
    | < RETURN: "return" >
    | < SUPER: "super" >
    /*novos itens */
    | < WHILE: "while" >
    | < DO: "do" >
    | < SWITCH: "switch" >
    | < CASE: "case" >
    | < DEFAULTT: "default" >
}

/* Tipos de variaveis */
TOKEN:
{
  < VAR_TYPE: (<INT> | <STRING> | <DOUBLE> | <CHAR> | <LONG> | <SHORT> | <BOOL> | <FLOAT> | <BYTE>) >
| < #INT:"int" >
| < #STRING:"string" >
| < #DOUBLE:"double" >
| < #CHAR:"char" >
| < #LONG:"long" >
| < #SHORT:"short" >
| < #BOOL:"boolean" >
| < #FLOAT:"float" >
| < #BYTE:"byte" >
}

/* constantes */

TOKEN :
{
    < byte_constant: (<DIGIT>)+ ["b", "B"] >
    |
    < short_constant: (<DIGIT>)+ ["s", "S"] >
    |
    < int_constant:(  
            ( (<DIGIT>)+ ) // números decimais 
            | (["0"-"7"] (["0"-"7"])* ["o", "O"] ) // octais 
            | ((<DIGIT> | ["A"-"F","a"-"f"])* ["h", "H"] ) //Hexadecimais 
            | (["0"-"1"] (["0"-"1"])* ["b", "B"]) // binarios
    ) >
    |
    < long_constant:( (<DIGIT>)+ ["l", "L"] ) >
    |
    < float_constant:( (<DIGIT>)* "." (<DIGIT>)+ ["f","F"]) >
    |
    < double_constant:( (<DIGIT>)* "." (<DIGIT>)+ ) >
    |
    < char_constant:
    "'" ( ["a"-"f","A"-"F"] | <DIGIT> ) "'" >
    |
    < string_constant:    
    "\""( ~["\"","\n","\r"])* "\"" >
    |
    < bool_constant: (<TRUE> | <FALSE>) >
    |
    < #TRUE: "true" >
    |
    < #FALSE: "false" >
    |
    < null_constant: "null" > 
}

/* Modificadores */
TOKEN:
{
  < MODIFIER: (<PUBLIC>|<PROTECTED>|<PRIVATE>)* >
  | < #PUBLIC:"public" >
  | < #PROTECTED:"protected" >
  | < #PRIVATE:"private" >
}


/* Definição do final */
TOKEN:
{
  < FINAL: "final" >
}

/* Identificadores */

TOKEN :
{
    < IDENT: <LETTER> (<LETTER>|<DIGIT>)* >
        |
        < #LETTER:["A"-"Z","a"-"z"] >
        |
        < #DIGIT:["0"-"9"] >
}


/* Simbolos especiais */

TOKEN :
{
    < LPAREN: "(" >
    | < RPAREN: ")" >
    | < LBRACE: "{" >
    | < RBRACE: "}" >
    | < LBRACKET: "[" >
    | < RBRACKET: "]" >
    | < SEMICOLON: ";" >
    | < COMMA: "," >
    | < DOT: "." >
    | < TODOT: ":" >
}

/* Operadores */

TOKEN :
{
  < ASSIGN: "=" >
  | < GT: ">" >
  | < LT: "<" >
  | < EQ: "==" >
  | < LE: "<=" >
  | < GE: ">=" >
  | < NEQ: "!=" >
  | < PLUS: "+" >
  | < MINUS: "-" >
  | < STAR: "*" >
  | < SLASH: "/" >
  | < REM: "%" >
  /*Adicionado para o trabalho*/
  | < AND: "&&">
  | < OR: "||">
  | < XOR: "^">
  | < NOT: "!">
}
/* Trata os erros léxicos */
SPECIAL_TOKEN :
{
    <INVALID_LEXICAL: 
        (~ ["a"-"z", "A"-"Z",
         "0"-"9",
         "\"",
         "(",
         ")",
         "[",
         "]",
         "{",
         "}",
         ";",
